{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "object-curly-newline": 0,
    "no-console": 1,
    "no-param-reassign": 1,
    "react/prop-types": 1,
    "no-unused-vars": 1
  }
}
